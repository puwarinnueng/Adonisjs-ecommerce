    <script>
        async function getApi() {
            let results = await fetch('https://api-partial.carmana.com/api/holiday_banner');
            let datas = await results.text();
            return datas;
        }
    </script>
    <script>
        const checkHoliday = async () => {
            let dataFromApi = !!(await getApi())?.length;

            let starttime = new Date();
            let endtime = new Date();
            let current = new Date();
            //วันเสาร์
            if (current.getDay() === 6) {
                starttime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 9, 0, 0, 0);
                endtime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 18, 30, 0, 0);
            }
            //วันปกติ
            if (current.getDay() > 0 && current.getDay() < 6) {
                starttime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 9, 0, 0, 0);
                endtime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 19, 0, 0, 0);
            }
            // คำนวนเวลา
            if ((current.getDay() > 0 && current > starttime && current < endtime) && dataFromApi === false) {
                console.log('เวลาทำการ');
                window.fbAsyncInit = function () {
                    FB.init({
                        appId: '232384267668484',
                        autoLogAppEvents: true,
                        xfbml: true,
                        version: 'v3.2'
                    });
                };

                (function (d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                    js = d.createElement(s);
                    js.id = id;
                    js.src = 'https://connect.facebook.net/th_TH/sdk/xfbml.customerchat.js';
                    fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));
            } else {
                console.log('นอกเวลาทำการ');
                document.getElementById("fb-root").remove();
            }
        }

        checkHoliday();
    </script>
