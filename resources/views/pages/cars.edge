        }

                
        function addQuery(url, query, value, oldVaue = '', force = false) {
            url +=  String(url).includes('?') ? '' : '?'
            if (force){
                return `${url}&${query}=${value}`
            }
            if (String(url).includes(`${query}=${oldVaue}`)){
                let regexForReplace = new RegExp(`${query.replace("[","\\[").replace("]","\\]") }=${oldVaue || '.*?' }(&|$)`)
                return String(url).replace(regexForReplace, `&${query}=${value}&`).replace(/\&\&*/g, '&').replace(/\&$/g, '')
            }
            return `${url}&${query}=${value}`.replace(/\&\&*/g, '&').replace(/\&$/g, '')
        }

        function removeQuery(queryName, value = '', removeAll = false) {
            let url = window.location.pathname + window.location.search
            let regexForReplace = new RegExp(`${queryName.replace("[",'\\[').replace("]","\\]") }=${value || '.*?' }(&|$)`, removeAll ? 'g' : '')
            console.log(String(regexForReplace))
            return String(url).replace(regexForReplace, '')
        }
    </script>

    

